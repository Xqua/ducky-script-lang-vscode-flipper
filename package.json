{
    "publisher": "Xqua",
    "name": "ducky-script-lang-vscode-flipper",
    "displayName": "Ducky Script Language - Flipper Zero mods - for VSCode",
    "description": "A simple syntax highlighting extension to enable support of Ducky Script as supported by the Flipper Zero. (forked from cvbenur)",
    "version": "1.0.5",
    "icon": "img/icon.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/Xqua/ducky-script-lang-vscode-flipper/"
    },
    "engines": {
        "vscode": "^1.42.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "duckyscript-flipper",
                "aliases": [
                    "Ducky Script Flipper Zero",
                    "duckyscript-flipper"
                ],
                "extensions": [
                    ".txt"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "duckyscript-flipper",
                "scopeName": "source.duckyscript-flipper",
                "path": "./syntaxes/duckyscript-flipper.tmLanguage.json"
            }
        ]
    }
}
